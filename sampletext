export secretName="/adt/frfs/cmc/dev/grafana-api-token"

secretArn=$(aws secretsmanager list-secrets \
  --region us-gov-east-1 \
  --query "SecretList[?Name=='$secretName'].ARN | [0]" \
  --output text)

export TF_VAR_grafana_auth=$(aws secretsmanager get-secret-value \
  --secret-id "$secretArn" \
  --region us-gov-east-1 \
  --query 'SecretString' \
  --output text)