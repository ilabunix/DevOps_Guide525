if opt_type == "call" and delta and ask:
    signals.append({
        "ticker": ticker,
        "strike": option["details"]["strike_price"],
        "expiry": option["details"]["expiration_date"],
        "entry": ask,
        "volume": volume,
        "oi": oi,
        "delta": delta,
        "iv_rank": 21,
        "flow_score": 75,
        "type": "intraday",
        "vwap_breakout": True
    })