function handler(event) {
  var request = event.request;
  var uri = request.uri;

  // Redirect for /distribution/client/fla/*
  if (uri.startsWith('/distribution/client/fla')) {
    return {
      statusCode: 301,
      statusDescription: 'Moved Permanently',
      headers: {
        location: { value: 'https://downloads.uat.frbservices.org/fla' }
      }
    };
  }

  // Redirect for /distribution/client/flw/*
  if (uri.startsWith('/distribution/client/flw')) {
    return {
      statusCode: 301,
      statusDescription: 'Moved Permanently',
      headers: {
        location: { value: 'https://downloads.uat.frbservices.org/flw' }
      }
    };
  }

  // For all other paths, continue normal processing
  return request;
}